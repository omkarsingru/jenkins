node{
     stage ('SCM Checkout') {
 git 'https://github.com/omkarsingru/jenkins'
 }
 stage ('Compile-Package'){
def mvn = tool (name: 'maven', type: 'maven') + '/bin/mvn'
 bat "${mvn} package"
 }
 stage ('test'){
 def mvn = tool (name: 'maven', type: 'maven') + '/bin/mvn'
 bat "${mvn} test"
 }
 stage( 'deploy'){
          deploy adapters: [tomcat7(credentialsId: 'c92a7606-7151-4869-90d9-67833ea760a5', url: 'http://localhost:8081')], contextPath: 'abcd', war: 'target/*.war'
}
 }
